
public class testeCargo {

	public static void main(String[] args) {
		
		FuncionarioParaTeste funcionario = new Funcionario();
		funcionario.setSalario(3000);
		System.out.println(funcionario.getTipo());
		System.out.println(funcionario.getBonificacao());
		
		FuncionarioParaTeste funcionario2 = new Funcionario();
		funcionario.setSalario(5000);
		funcionario.setTipo(1);
		System.out.println(funcionario2.getTipo());
		System.out.println(funcionario2.getBonificacao());

	}

}
